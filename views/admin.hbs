<br>
<div class="container">
    <h1> Administrados de Emails</h1>

    <div class="row">

        {{#each emails}}
        <div style="padding:20px ;" class="col s12 l6 m12 itemEmail ">
            <strong>
                <span>Email:</span>
            </strong>
            {{email }}
            <br>
            <strong>
                <span>Nombre:</span>
            </strong>
            {{ nombre}}

            <div class="idEmail">
                {{ID}}
            </div>

            <div class="input-field ">
                <textarea class="materialize-textarea"> {{ contenido }}</textarea>
            </div>

            <div class="deleteById" id={{ID}}>

                <i class="fa fa-trash delete" id={{ID}} aria-hidden="true"></i>
            </div>
        </div>
        {{/each}}
    </div>
</div>

<script>

    //''
    $('.delete').click(e => {
        console.log(e.target)

        const id = e.target.id;
        alert(id);
    });
</script>